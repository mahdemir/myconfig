[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
[user]
	name = mahdemir
	email = mahdemir@users.noreply.github.com
[push]
	default = current
[alias]
	co = checkout
	s = status
	sw = switch
	b = branch
	ba = branch -a
	com = commit -m
	alias = config --get-regexp ^alias\\.
	cl = clone
	ls = ls-files
	last = log -1 HEAD
	cll = clone --depth 1
